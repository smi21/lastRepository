package com.mccoy;


public class Pojo 
{
private String name;
private String password;

public Pojo() {
	// TODO Auto-generated constructor stub
}

public String getName() {
	return name;
}

public void setName(String name) {
	this.name = name;
}

public String getPassword() {
	return password;
}

public void setPasword(String pasword) {
	this.password = pasword;
}


}
